{
  "name": "notion-doit",
  "version": "0.0.1",
  "description": "Use Notion as the CMS for your Hugo DoIt site",
  "main": "src/index.ts",
  "private": true,
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/Hugo-DoIt/Notion-DoIt",
  "bugs": {
    "url": "https://github.com/Hugo-DoIt/Notion-DoIt/issues"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "start": "tsx src/index.ts",
    "server": "hugo server -D --disableFastRender --noHTTPCache",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "Notion",
    "Hugo",
    "theme",
    "DoIt"
  ],
  "author": "HEIGE-PCloud",
  "dependencies": {
    "@notionhq/client": "^4.0.1",
    "dotenv": "^17.2.1",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.3.0",
    "markdown-table": "^3.0.4",
    "tsx": "^4.20.3",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250801.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
